    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "autoprefixer": "^10.2.5",
    "eslint": "<8.0.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwindcss": "^1.17.0",
